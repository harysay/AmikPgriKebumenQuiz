<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="com.harysaydev.amikpgrikbmquiz.Navigation_Activity"
    tools:showIn="@layout/app_bar_navigation_">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">
            <Button
                android:id="@+id/idkebangsaan"
                android:layout_width="0dp"
                android:layout_height="200dp"
                android:layout_margin="2dp"
                android:layout_weight="1"
                android:background="#524d47"
                android:foreground="?attr/selectableItemBackground"
                android:drawableTop="@drawable/pancasila"
                android:paddingTop="25dp"
                android:text="TWK (Kebangsaan)"
                android:textAllCaps="false"
                android:textColor="#ffffff"
                android:textSize="18sp" />
            <Button

                android:id="@+id/idintelegensi"
                android:layout_width="0dp"
                android:layout_height="200dp"
                android:layout_margin="2dp"
                android:layout_weight="1"
                android:background="#63c28c"
                android:foreground="?attr/selectableItemBackground"
                android:drawableTop="@drawable/maths"
                android:paddingTop="25dp"
                android:text="TIU (Intelegensi)"
                android:textAllCaps="false"
                android:textColor="#fff" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">
            <Button
                android:id="@+id/idkepribadian"
                android:layout_width="0dp"
                android:layout_height="200dp"
                android:layout_margin="2dp"
                android:layout_weight="1"
                android:background="#f71735"
                android:drawableLeft="@drawable/capital"
                android:paddingLeft="35dp"
                android:foreground="?attr/selectableItemBackground"
                android:text="TKP (Kepribadian)"

                android:textAllCaps="false"
                android:textColor="#fff"
                android:textSize="18sp" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">
            <Button
                android:id="@+id/idkomputer"
                android:layout_width="0dp"
                android:layout_height="200dp"
                android:layout_margin="2dp"
                android:layout_weight="1"
                android:background="#f0eb79"
                android:foreground="?attr/selectableItemBackground"
                android:drawableTop="@drawable/computer"
                android:paddingTop="25dp"
                android:text="Komputer"
                android:textAllCaps="false"
                android:textColor="#2f2e2e"
                android:textSize="18sp" />

            <Button
                android:id="@+id/idprofesi"
                android:layout_width="0dp"
                android:layout_height="200dp"
                android:layout_margin="2dp"
                android:layout_weight="1"
                android:background="#88a6db"
                android:drawableTop="@drawable/profesional"
                android:paddingTop="25dp"
                android:text="Profesi"
                android:foreground="?attr/selectableItemBackground"
                android:textAllCaps="false"
                android:textColor="#fff" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

                <Button
                    android:id="@+id/idinggris"
                    android:layout_width="0dp"
                    android:layout_height="200dp"
                    android:layout_margin="2dp"
                    android:foreground="?attr/selectableItemBackground"
                    android:layout_weight="1"
                    android:background="#d9dbd0"
                    android:drawableTop="@drawable/english"
                    android:paddingTop="40dp"
                    android:text="Bahasa Inggris"
                    android:textAllCaps="false"
                    android:textColor="#2f2e2e"
                    android:textSize="18sp" />

            <Button
                android:id="@+id/idpengetahuanumum"
                android:layout_width="0dp"
                android:layout_height="200dp"
                android:layout_margin="2dp"
                android:layout_weight="1"
                android:background="#a084d1"
                android:drawableTop="@drawable/general"
                android:paddingTop="25dp"
                android:foreground="?attr/selectableItemBackground"
                android:text="Pengetahuan Umum"
                android:textAllCaps="false"
                android:textColor="#fff"
                android:textSize="18sp" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">
            <Button
                android:id="@+id/idekonomiakuntansi"
                android:layout_width="0dp"
                android:layout_height="200dp"
                android:layout_margin="2dp"
                android:layout_weight="1"
                android:background="#80e7a9"
                android:drawableTop="@drawable/pp"
                android:paddingTop="25dp"
                android:text="Ekonomi Akuntansi"
                android:textAllCaps="false"
                android:textColor="#2f2e2e"
                android:textSize="18sp" />


            <Button
                android:id="@+id/idliteratur"
                android:layout_width="0dp"
                android:layout_height="200dp"
                android:foreground="?attr/selectableItemBackground"
                android:layout_margin="2dp"
                android:layout_weight="1"
                android:background="#cc8d66"
                android:drawableTop="@drawable/books"
                android:paddingTop="25dp"
                android:text="Metodologi Penelitian"
                android:textAllCaps="false"
                android:textColor="#fff"
                android:textSize="18sp" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">
            <Button
                android:id="@+id/idkampus"
                android:layout_width="0dp"
                android:foreground="?attr/selectableItemBackground"
                android:layout_height="200dp"
                android:layout_margin="2dp"
                android:layout_weight="1"
                android:background="#e7595d"
                android:drawableLeft="@drawable/invention"
                android:paddingLeft="45dp"
                android:text="Pengetahuan AMIK PGRI Kebumen"
                android:textAllCaps="false"
                android:textColor="#fff"
                android:textSize="18sp" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">
            <Button
                android:id="@+id/idgrafikom"
                android:layout_width="0dp"
                android:layout_height="200dp"
                android:layout_margin="2dp"
                android:layout_weight="1"
                android:background="#f797f7"
                android:drawableTop="@drawable/grafika96x96"
                android:paddingTop="25dp"
                android:text="Grafika Komputer"
                android:textAllCaps="false"
                android:textColor="#2f2e2e"
                android:textSize="18sp" />


            <Button
                android:id="@+id/idpbo"
                android:layout_width="0dp"
                android:layout_height="200dp"
                android:foreground="?attr/selectableItemBackground"
                android:layout_margin="2dp"
                android:layout_weight="1"
                android:background="#c7c112"
                android:drawableTop="@drawable/pbo96x96"
                android:paddingTop="25dp"
                android:text="PBO"
                android:textAllCaps="false"
                android:textColor="#2f2e2e"
                android:textSize="18sp" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">
            <Button
                android:id="@+id/idkecerdasan"
                android:layout_width="0dp"
                android:layout_height="200dp"
                android:layout_margin="2dp"
                android:layout_weight="1"
                android:background="#f2edf2"
                android:drawableTop="@drawable/kecerdasanbuatan96x96"
                android:paddingTop="25dp"
                android:text="Kecerdasan Buatan"
                android:textAllCaps="false"
                android:textColor="#2f2e2e"
                android:textSize="18sp" />
            <Button
                android:id="@+id/idrpl"
                android:layout_width="0dp"
                android:layout_height="200dp"
                android:foreground="?attr/selectableItemBackground"
                android:layout_margin="2dp"
                android:layout_weight="1"
                android:background="#b2ead6"
                android:drawableTop="@drawable/rpl96x96"
                android:paddingTop="25dp"
                android:text="RPL"
                android:textAllCaps="false"
                android:textColor="#2f2e2e"
                android:textSize="18sp" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">
            <Button
                android:id="@+id/idstrukturdata"
                android:layout_width="0dp"
                android:layout_height="200dp"
                android:layout_margin="2dp"
                android:layout_weight="1"
                android:background="#FFC107"
                android:drawableTop="@drawable/strukturdata"
                android:paddingTop="25dp"
                android:text="Struktur Data"
                android:textAllCaps="false"
                android:textColor="#2f2e2e"
                android:textSize="18sp" />


            <Button
                android:id="@+id/idpemrogramaninternet"
                android:layout_width="0dp"
                android:layout_height="200dp"
                android:foreground="?attr/selectableItemBackground"
                android:layout_margin="2dp"
                android:layout_weight="1"
                android:background="#8BC34A"
                android:drawableTop="@drawable/pemroginternet"
                android:paddingTop="25dp"
                android:text="Pemrog. Internet"
                android:textAllCaps="false"
                android:textColor="#2f2e2e"
                android:textSize="18sp" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">
            <Button
                android:id="@+id/idjarkom"
                android:layout_width="0dp"
                android:layout_height="200dp"
                android:layout_margin="2dp"
                android:layout_weight="1"
                android:background="#DCEDC8"
                android:drawableTop="@drawable/jarkom"
                android:paddingTop="25dp"
                android:text="Jaringan Komputer"
                android:textAllCaps="false"
                android:textColor="#2f2e2e"
                android:textSize="18sp" />

            <Button
            android:id="@+id/iddesainweb"
            android:layout_width="0dp"
            android:layout_height="200dp"
            android:foreground="?attr/selectableItemBackground"
            android:layout_margin="2dp"
            android:layout_weight="1"
            android:background="#FB7A7E"
            android:drawableTop="@drawable/desainweb"
            android:paddingTop="25dp"
            android:text="Desain Web"
            android:textAllCaps="false"
            android:textColor="#2f2e2e"
            android:textSize="18sp" />

    </LinearLayout>

